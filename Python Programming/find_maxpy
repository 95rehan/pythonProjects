# Python program to find a largest element

sample_input = [2,5,6,7,8,9,4,5,1,2,5,7,20]


def find_max(lst : list) -> int:
    return max(lst)

find_max(sample_input)


# Other way not recommended


def find_max2(lst: list) -> int:
    mx = 0
    for i in lst:
        if i > mx:
            mx = i

    return mx

find_max2(sample_input)